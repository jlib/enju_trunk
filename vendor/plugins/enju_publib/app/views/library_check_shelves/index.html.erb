<div id="content_detail" class="ui-corner-all">
<h1 class="title"><%= t('page.new', :model => t('activerecord.models.library_check_shelf')) -%> </h1>
<div id="content_list">

<%= form_for(@library_check, :html => { :multipart => true }) do |f| -%> 
  <%= f.error_messages -%> 

  <p>
    <%= f.label t('page.file') -%><br />
    <%= f.file_field :shelf_upload -%>
  </p>

  <p>
    <%= f.submit %>
  </p>
<%- end -%>

</div>
</div>

<div id="submenu" class="ui-corner-all">
  <ul>
    <li><%= link_to t('page.back'), library_check_path(@library_check) -%> </li>
  </ul>
</div>
